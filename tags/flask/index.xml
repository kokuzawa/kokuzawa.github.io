&lt;pre class=&#34;highlight&#34;&gt;&lt;code class=&#34;language-py&#34; data-lang=&#34;py&#34;&gt;from bottle import run, get


@get(&#39;/hello/&amp;lt;name&amp;gt;&#39;)
def hello(name):
    return &#39;Hello %s&#39; % name


run(host=&#39;localhost&#39;, port=8081, debug=True, reloader=True)&lt;/code&gt;&lt;/pre&gt;
